@model List<CrudCore.Models.StudentMod>


@using Microsoft.AspNetCore.Http
@inject Microsoft.AspNetCore.Http.IHttpContextAccessor HttpContextAccessor

<h1>**************************** Session **************************</h1>
<h1>@HttpContextAccessor.HttpContext.Session.GetString("name")</h1>

@{
    ViewData["Title"] = "Home Page";
}
<table class="table table-dark">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Email</th>
        <th>Password</th>
        <th>Edit</th>
        <th>Delete</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Email</td>
            <td>@item.Password</td>
            <td><a asp-action="Edit" asp-route-id="@item.Id" class="btn btn-info">Edit</a></td>
            <td><a asp-action="Delete" asp-route-id="@item.Id" class="btn btn-danger">Delete</a></td>
        </tr>
    }
</table>
